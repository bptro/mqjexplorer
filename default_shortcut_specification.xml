<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
  <skipIfNotSupported />

  <programGroup defaultName="MQJExplorer" location="applications"/>

   <shortcut
     name="MQJExplorer"
     programGroup="no"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\runme.bat"
     commandLine=""
     description="MQJExplorer $APP_VER"
     iconFile="$INSTALL_PATH/icons/logo.ico"
     iconIndex="0">
     <createForPack name="MQJExplorer" />
   </shortcut>

   <shortcut
     name="MQJExplorer Uninstaller"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$JAVA_HOME\bin\java"
     commandLine="-jar $INSTALL_PATH\Uninstaller\uninstaller.jar"
     iconFile="%SystemRoot%\system32\SHELL32.dll"
     iconIndex="31"
     description="This uninstalls MQJExplorer $APP_VER">
     <createForPack name="MQJExplorer" />
   </shortcut>

</shortcuts>
